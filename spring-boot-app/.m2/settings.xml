<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>maven-releases</id> <!-- This should match the <id> defined in the <distributionManagement> section of your POM.xml -->
            <username>admin</username> <!-- Nexus username -->
            <password>admin123</password> <!-- Nexus password -->
        </server>
        <server>
            <id>maven-snapshots</id> <!-- This should match the <id> defined in the <distributionManagement> section of your POM.xml -->
            <username>admin</username> <!-- Nexus username -->
            <password>admin123</password> <!-- Nexus password -->
        </server>
    </servers>
</settings>